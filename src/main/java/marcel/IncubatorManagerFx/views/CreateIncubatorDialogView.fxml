                  <Font size="27.0" />
               </font>
            </Label>
            <TextField fx:id="txName" layoutX="279.0" layoutY="117.0" prefHeight="25.0" prefWidth="256.0" />
            <TextField fx:id="txUniqueCode" layoutX="279.0" layoutY="159.0" prefHeight="25.0" prefWidth="256.0" />
            <TextField fx:id="txLocation" layoutX="279.0" layoutY="201.0" prefHeight="25.0" prefWidth="256.0" />
            <Label layoutX="90.0" layoutY="116.0" text="Name:">
               <font>
                  <Font size="19.0" />
               </font>
            </Label>
            <Label layoutX="90.0" layoutY="158.0" text="Unique code:">
               <font>
                  <Font size="19.0" />
               </font>
            </Label>
            <Label layoutX="92.0" layoutY="196.0" text="Location:">
               <font>
                  <Font size="19.0" />
               </font>
            </Label>
            <Label layoutX="90.0" layoutY="242.0" text="Description:">
               <font>
                  <Font size="19.0" />
               </font>
            </Label>
            <TextArea fx:id="txDescription" layoutX="279.0" layoutY="242.0" prefHeight="111.0" prefWidth="256.0" />
