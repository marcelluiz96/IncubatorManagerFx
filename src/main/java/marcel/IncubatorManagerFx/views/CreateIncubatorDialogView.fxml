<AnchorPane stylesheets="@../../../resources/css/style.css" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="marcel.IncubatorManagerFx.views.CreateIncubatorDialogController">
   <children>
      <AnchorPane prefHeight="480.0" prefWidth="640.0">
         <children>
            <Label layoutX="192.0" layoutY="14.0" text="Create new Incubator">
