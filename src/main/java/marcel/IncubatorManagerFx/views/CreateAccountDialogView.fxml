<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane stylesheets="@../../../resources/css/style.css" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="marcel.IncubatorManagerFx.views.CreateAccountDialogController">
   <children>
      <AnchorPane prefHeight="480.0" prefWidth="640.0">
         <children>
            <Label layoutX="192.0" layoutY="14.0" text="Create new Account">
               <font>
                  <Font size="27.0" />
               </font>
            </Label>
            <TextField fx:id="txLogin" layoutX="279.0" layoutY="141.0" prefHeight="25.0" prefWidth="256.0" />
            <Label layoutX="107.0" layoutY="140.0" text="Login">
               <font>
                  <Font size="19.0" />
               </font>
            </Label>
            <Label layoutX="107.0" layoutY="187.0" text="Password">
               <font>
                  <Font size="19.0" />
               </font>
            </Label>
            <Button fx:id="btSave" layoutX="332.0" layoutY="343.0" mnemonicParsing="false" onAction="#saveAccount" prefHeight="45.0" prefWidth="111.0" styleClass="greenbt" text="Save">
               <font>
                  <Font size="21.0" />
               </font>
            </Button>
            <Button fx:id="btCancel" layoutX="157.0" layoutY="343.0" mnemonicParsing="false" onAction="#cancel" prefWidth="111.0" styleClass="redbt" text="Cancel">
               <font>
                  <Font size="21.0" />
               </font>
            </Button>
            <Label layoutX="107.0" layoutY="235.0" text="Confirm Password">
               <font>
                  <Font size="19.0" />
               </font>
            </Label>
            <PasswordField fx:id="txPassword" layoutX="279.0" layoutY="189.0" prefHeight="25.0" prefWidth="256.0" />
            <PasswordField fx:id="txConfirmPassword" layoutX="279.0" layoutY="237.0" prefHeight="25.0" prefWidth="256.0" />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
