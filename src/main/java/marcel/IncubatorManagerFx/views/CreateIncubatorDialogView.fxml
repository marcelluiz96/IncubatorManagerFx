               <font>
                  <Font size="21.0" />
               </font>
            </Button>
            <Button fx:id="btCancel" layoutX="146.0" layoutY="427.0" mnemonicParsing="false" onAction="#cancel" prefWidth="111.0" styleClass="redbt" text="Cancel">
               <font>
                  <Font size="21.0" />
               </font>
            </Button>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
